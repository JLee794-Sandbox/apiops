{
  "properties": {
    "circuitBreaker": {
      "rules": [
        {
          "acceptRetryAfter": true,
          "failureCondition": {
            "count": 1,
            "errorReasons": [
              "Server errors"
            ],
            "interval": "PT5M",
            "statusCodeRanges": [
                {
                "max": 429,
                "min": 429
              }
            ]
          },
          "name": "openAIBreakerRule",
          "tripDuration": "PT1M"
        }
      ]
    },
    "credentials": {
      "header": {},
      "query": {}
    },
    "protocol": "http",
    "tls": {
      "validateCertificateChain": true,
      "validateCertificateName": true
    },
    "url": "https://aoai-jlee-eus.openai.azure.com/openai"
  }
}